{
  "manifest_version": 2,

  "name": "Emoji Input",
  "description": "Allow input emoji in every website",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Emoji Input"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}